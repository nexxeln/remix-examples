{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:remix": "remix build",
    "build:css": "unocss './app/**/*.{js,ts,jsx,tsx}' -o ./app/styles/uno.css",
    "dev": "run-p \"dev:*\"",
    "dev:remix": "remix dev",
    "dev:css": "unocss './app/**/*.{js,ts,jsx,tsx}' -o ./app/styles/uno.css --watch",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@remix-run/node": "*",
    "@remix-run/react": "*",
    "@remix-run/serve": "*",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "*",
    "@remix-run/eslint-config": "*",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@unocss/cli": "^0.46.4",
    "@unocss/reset": "^0.46.4",
    "eslint": "^8.10.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.7.4",
    "unocss": "^0.46.4"
  },
  "engines": {
    "node": ">=14"
  }
}
